{
  "name": "vue-google-adsense",
  "description": "Vue.js Google Adsense Component with InFeed and InArticle Ads support",
  "version": "1.3.0",
  "author": "Irfan Maulana <mazipanneh@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=developmet ./node_modules/.bin/poi --serve",
    "build-demo": "rimraf demo && cross-env NODE_ENV=production ./node_modules/.bin/poi --prod",
    "build-lib": "rimraf dist && ./node_modules/.bin/bili --config bili.config.js",
    "dist": "npm run build-demo && npm run build-lib",
    "lint": "eslint --ext .js,.vue --ignore-path .eslintignore .",
    "lint-fix": "eslint --ext .js,.vue --ignore-path .eslintignore . --fix"
  },
  "main": "dist/vuegoogleadsense.dist.js",
  "files": [
    "dist/vuegoogleadsense.dist.js"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mazipan/vue-google-adsense.git"
  },
  "bugs": {
    "url": "https://github.com/mazipan/vue-google-adsense/issues"
  },
  "keywords": [
    "vuejs adsense",
    "vue2 adsense",
    "vue adsense",
    "vuejs google adsense",
    "vue2 google adsense",
    "vue google adsense"
  ],
  "dependencies": {
    "vue-script2": "2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "bili": "^3.4.2",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.1.0",
    "file-loader": "^3.0.1",
    "gh-pages": "2.0.1",
    "jest": "^23.6.0",
    "jest-vue": "^0.8.2",
    "mustache": "^3.0.1",
    "node-sass": "^4.11.0",
    "ora": "^3.0.0",
    "poi": "12.4.2",
    "rimraf": "^2.6.3",
    "rollup-plugin-vue": "^4.3.2",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.3",
    "style-loader": "0.23.1",
    "svgo": "^1.1.1",
    "vue": "^2.5.21",
    "vue-loader": "^15.5.1",
    "vue-template-compiler": "^2.5.21",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "eslintIgnore": [
    "test/*.js",
    "dist/*.js",
    "node_modules"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended"
    ]
  },
  "jest": {
    "mapCoverage": true,
    "coverageDirectory": "test/coverage",
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue"
    }
  }
}
