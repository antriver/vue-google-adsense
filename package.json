{
  "name": "vue-google-adsense",
  "description": "Vue.js Google Adsense Component with InFeed and InArticle Ads support",
  "version": "1.4.5",
  "author": "Irfan Maulana <mazipanneh@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev:webpack": "cross-env NODE_ENV=development webpack-dev-server --hot --color --open --config webpack.config.demo.js",
    "dev": "cross-env NODE_ENV=developmet ./node_modules/.bin/poi --serve",
    "build:demo": "rimraf demo && cross-env NODE_ENV=production ./node_modules/.bin/poi --prod",
    "build:lib": "rimraf dist && ./node_modules/.bin/bili --config bili.config.js",
    "build:webpack": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:webpack-demo": "rimraf demo && cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack.config.demo.js",
    "build": "npm run build:demo && npm run build:lib",
    "lint": "eslint --ext .js,.vue --ignore-path .eslintignore .",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .eslintignore . --fix"
  },
  "main": "dist/VueGoogleAdsense.cjs.min.js",
  "module": "dist/VueGoogleAdsense.cjs.min.js",
  "unpkg": "dist/VueGoogleAdsense.umd.min.js",
  "jsdelivr": "dist/VueGoogleAdsense.umd.min.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mazipan/vue-google-adsense.git"
  },
  "bugs": {
    "url": "https://github.com/mazipan/vue-google-adsense/issues"
  },
  "keywords": [
    "vuejs adsense",
    "vue2 adsense",
    "vue adsense",
    "vuejs google adsense",
    "vue2 google adsense",
    "vue google adsense"
  ],
  "dependencies": {
    "vue": "^2.6.7",
    "vue-script2": "2.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "bili": "4.4.0",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "poi": "12.5.5",
    "rimraf": "^2.6.3",
    "rollup-plugin-vue": "4.7.2",
    "sass-loader": "^7.1.0",
    "style-loader": "0.23.1",
    "vue-loader": "^15.6.4",
    "vue-template-compiler": "^2.6.7",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-serve": "^2.0.3",
    "workbox-webpack-plugin": "^3.6.3"
  }
}
